.hummingbird-treeview,
.hummingbird-treeview * {
    padding-top: 3px;
    padding-bottom: 3px;
    list-style: none;
    line-height: 16px;
}

.hummingbird-treeview label {
    font-weight: normal;
}

.hummingbird-treeview input[type=checkbox] {
    width: 16px;
    height: 16px;
    padding: 0px;
    margin: 0px;
}

.hummingbird-treeview ul.collapsable {
    display: none;
}

.hummingbird-treeview .fa {
    font-style: normal;
    cursor: pointer;
}

