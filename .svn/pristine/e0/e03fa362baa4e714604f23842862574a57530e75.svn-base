<!-- begin row -->
<div class="row">
	
    <!-- PURCHASE FIRST LEVEL PANEL START -->
    <div class="col-md-12" id="purchase_first_level_panel_front" style="display: block;">
        <div class="{{setting.constant.panel_color}}">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i
                            class="fa fa-expand"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i
                            class="fa fa-repeat"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning"
                       data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger"
                       data-click="panel-remove"><i class="fa fa-times"></i></a>
                </div>
                <div class="btn-group pull-right m-r-10">

                </div>
                <h4 class="panel-title ttc" do_back_button>
                    {{first_panel_title.label}}
                </h4>
            </div>
            <div class="panel-body">
                <div id="purchase_first_level_toolbar">
                    <div class="form-inline" action="/" method="POST">
                        <div class="form-group m-r-10">
                            <select class="form-control input-sm">
                                <option value="">Export Selected</option>
                                <option value="basic">Export Current</option>
                                <option value="all">Export All</option>
                            </select>
                        </div>
                        <button id="purchase_first_level_resetSearch" class="{{setting.constant.button}}">Reset search
                        </button>
                    </div>
                </div>
                <table class="table purchase_first_level_table">
                    <thead>
                    <tr>
                        <th ng-repeat="header in table_headers" class="ttc" ng-class="(header == 'Each_Value' || header == 'Total_Value' || header == 'Quantity' || header == 'Pending_GRN_Value') ? 'text-right' : ''"
                            data-field='{{header}}'>{{header.replace("_", " ")}}
                        </th>
                    </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <!-- PURCHASE FIRST LEVEL PANEL FRONT END -->

    <!-- PURCHASE SECOND LEVEL PANEL START -->

    <div class="col-md-12" id="purchase_second_level_panel_front" ng-show="purchase_second_level_panel_front_show">
        <div class="{{setting.constant.panel_color}}">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i
                            class="fa fa-expand"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i
                            class="fa fa-repeat"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning"
                       data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger"
                       data-click="panel-remove"><i class="fa fa-times"></i></a>
                </div>
                <div class="btn-group pull-right m-r-10">

                </div>
                <h4 class="panel-title ttc">
                    {{second_panel_title.label}}
                </h4>
            </div>
            <div class="panel-body">
                <div id="purchase_second_level_toolbar">
                    <div class="form-inline" action="/" method="POST">
                        <div class="form-group m-r-10">
                            <select class="form-control input-sm">
                                <option value="">Export Selected</option>
                                <option value="basic">Export Current</option>
                                <option value="all">Export All</option>
                            </select>
                        </div>
                        <button id="purchase_second_level_resetSearch" class="{{setting.constant.button}}">Reset search
                        </button>
                    </div>
                </div>
                <table class="table purchase_second_level_table">
                    <thead>
                    <tr>
						
						<th ng-repeat="header_second in table_headers_second" class="ttc" ng-class="(header_second == 'Quantity' || header_second == 'Each_Value' || header_second == 'Total_Value' || header_second == 'Pending_GRN_Value') ? 'text-right' : ''"
                            data-field='{{header_second}}'>{{header_second.replace("_", " ")}}
                        </th>
                    </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>

    <!-- PURCHASE SECOND LEVEL PANEL FRONT END -->

</div>
<!-- end row -->
